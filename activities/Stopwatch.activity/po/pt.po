#: TutoPrev
msgctxt "TutoPrev"
msgid "Prev"
msgstr "Anterior"

#: TutoNext
msgctxt "TutoNext"
msgid "Next"
msgstr "Próximo"

#: TutoEnd
msgctxt "TutoEnd"
msgid "End"
msgstr "Fim"

#: TutoExplainTitle
msgctxt "TutoExplainTitle"
msgid "Stopwatch Activity"
msgstr "Atividade Cronômetro"

#: TutoExplainContent
msgctxt "TutoExplainContent"
msgid "Welcome to the Stopwatch activity. Stopwatch is a simple stopwatch. It shows the user a number of digital stopwatches. Each stopwatch is independent"
msgstr "Welcome to the Stopwatch activity. Stopwatch is a simple stopwatch. It shows the user a number of digital stopwatches. Each stopwatch is independent"

#: TutoAddTitle
msgctxt "TutoAddTitle"
msgid "Add"
msgstr "Adicionar"

#: TutoAddContent
msgctxt "TutoAddContent"
msgid "Press this icon to add a new Stopwatch"
msgstr "Clique sobre este ícone para adicionar um novo cronômetro"

#: TutoCounterTitle
msgctxt "TutoCounterTitle"
msgid "Counter"
msgstr "Contador"

#: TutoCounterContent
msgctxt "TutoCounterContent"
msgid "Displays the current time of the Stopwatch"
msgstr "Exibir o tempo atual do cronômetro"

#: TutoStartStopTitle
msgctxt "TutoStartStopTitle"
msgid "Start/Stop"
msgstr "Start/Stop"

#: TutoStartStopContent
msgctxt "TutoStartStopContent"
msgid "Press this icon to Start/Stop the Stopwatch"
msgstr "Clique sobre o ícone para iniciar/parar o cronômetro"

#: TutoResetTitle
msgctxt "TutoResetTitle"
msgid "Reset"
msgstr "Reset"

#: TutoResetContent
msgctxt "TutoResetContent"
msgid "Press this icon to reset the Stopwatch"
msgstr "Clique sobre o ícone para reiniciar o cronômetro"

#: TutoMarkTitle
msgctxt "TutoMarkTitle"
msgid "Lap"
msgstr "Lap"

#: TutoMarkContent
msgctxt "TutoMarkContent"
msgid "Press this icon to add an intermediate time lap"
msgstr "Press this icon to add an intermediate time lap"

#: TutoRemoveTitle
msgctxt "TutoRemoveTitle"
msgid "Remove"
msgstr "Remover"

#: TutoRemoveContent
msgctxt "TutoRemoveContent"
msgid "Press this icon to remove this Stopwatch from list"
msgstr "Clique sobre o ícone para remover este cronômetro da lista"
