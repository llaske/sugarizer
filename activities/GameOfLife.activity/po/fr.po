msgid "Generation"
msgstr "Génération"

#: TutoPrev
msgctxt "TutoPrev"
msgid "Prev"
msgstr "Préc"

#: TutoNext
msgctxt "TutoNext"
msgid "Next"
msgstr "Suiv"

#: TutoEnd
