{
    "name": "Turtle Blocks",
    "version": "1.0",
    "manifest_version": 2,
    "author": "Sugar Labs",
    "description": "Learn to program anything with snap-together blocks.",
    "icons": {
	"512": "/activity/activity-icon-color-512.png",
	"128": "/activity/activity-icon-color-128.png"
    },
    "app": {
	"background": {
	    "scripts": ["js/launch.js"]
	}
    },
    "permissions": [
	"storage",
	"unlimitedStorage",
	"audioCapture",
	"videoCapture"
    ]
}
